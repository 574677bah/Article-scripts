  <DataSets>
    <DataSet Name="ServerListDataset">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT [Id], [MachineName]+'\'+[Name] as name
FROM [ApexSQLMonitor].[ApexSQL].[MonitoredObjectsView]
WHERE Type=2</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Top10ApplicationsWithHighestWaitTimeInDatabase">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SourceId">
            <Value>=Parameters!SourceId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Top 10 LI.Name as [Application], SUM (WaitTime)  as [Wait time] 
FROM [ApexSQLMonitor].[ApexSQL].[MonitorQueryWaits] QW LEFT JOIN 
[ApexSQLMonitor].[ApexSQL].[MonitorQueryLookupInfo] LI ON LI.Id = QW.ApplicationNameId
WHERE Li.Type = 3 AND QW.SourceId = @SourceId
GROUP BY LI.Name 
ORDER by [Wait time] DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Application">
          <DataField>Application</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Wait_time">
          <DataField>Wait time</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
