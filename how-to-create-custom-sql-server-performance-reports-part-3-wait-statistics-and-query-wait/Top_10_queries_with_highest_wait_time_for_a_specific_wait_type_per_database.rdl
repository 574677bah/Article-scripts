    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SourceId">
            <Value>=Parameters!SourceId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WaitType">
            <Value>=Parameters!WaitType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DatabaseId">
            <Value>=Parameters!DatabaseId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartTime">
            <Value>=Parameters!StartTime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndTime">
            <Value>=Parameters!EndTime.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP 10 
	 CONVERT (VARCHAR (MAX), QW.[SqlHandle], 2) as SqlHandle
	,MAX(QT.SqlText) as [T-SQL]
	,CONVERT (VARCHAR (MAX), QW.[PlanHandle], 2) as PlanHandle
	,SUM(QW.[WaitTime]) as [Total wait]
  FROM [ApexSQLMonitor].[ApexSQL].[MonitorQuerySingleWaitsView] QW 
  LEFT JOIN ApexSQL.[MonitorQueryTexts]  QT ON QW.QueryTextId = QT.Id
  LEFT JOIN ApexSQL.[MonitorDatabases] MD ON QW.DatabaseNameId = MD.Id
WHERE QW.SourceId = @SourceId AND WaitTypeName = @WaitType AND QT.SqlText IS NOT NULL AND QW.DatabaseNameId = @DatabaseId AND [MeasuredAt] &gt;= @StartTime AND [MeasuredAt]&lt;= @EndTime
  GROUP BY 
    QW.[QueryTextId] 
      ,QW.[SqlHandle]
      ,QW.[PlanHandle]
      ,QW.[StmOffsetStart]
      ,QW.[StmOffsetEnd]
   ,QW.[SourceId]
   ,QW.DatabaseNameId
   ,QW.[WaitTypeName]
   ORDER By [Total wait] DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="SqlHandle">
          <DataField>SqlHandle</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="T_SQL">
          <DataField>T-SQL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PlanHandle">
          <DataField>PlanHandle</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_wait">
          <DataField>Total wait</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ServerListDataset">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT [Id], [MachineName]+'\'+[Name] as name
FROM [ApexSQLMonitor].[ApexSQL].[MonitoredObjectsView]
WHERE Type=2</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DatabaseListDataset">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ServerId">
            <Value>=Parameters!SourceId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT [Id], [Name]
FROM [ApexSQLMonitor].[ApexSQL].[MonitoredObjectsView]
WHERE Type=3 AND ServerId = @ServerId
ORDER BY Name ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="WaitTypeListDataset">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT [Id],  [WaitTypeName]
       FROM [ApexSQLMonitor].[ApexSQL].[MonitorWaitTypes]
	   WHERE Excluded = 0
	   ORDER by WaitTypeName ASC</CommandText>
      </Query>
      <Fields>
