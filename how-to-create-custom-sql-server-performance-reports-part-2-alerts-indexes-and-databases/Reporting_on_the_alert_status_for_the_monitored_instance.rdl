  <DataSets>
    <DataSet Name="StatusOfAlertsDataset">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Severity">
            <Value>=Parameters!Severity.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@State">
            <Value>=Parameters!State.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SourceID">
            <Value>=Parameters!SourceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
		  ApexSQL.SourceIdToName(A.[SourceId]) as Source,
		  ApexSQL.MetricIdToName(A.[MeasurementId]) as Metric,
		  ApexSQL.SeverityToStringConverter(A.[Severity]) as Severity,
          [ApexSQL].[AlertCheckToStringConverter](A.[Checked]) as Reviewed,
          [ApexSQL].[AlertStateToStringConverter](A.[State]) as [State],
          A.[TimeRaised],
          MM.Value,
          A.[TimeResolved],
          A.[UserResolved]
  FROM [ApexSQLMonitor].[ApexSQL].[MonitorAlerts] A 
  LEFT JOIN ApexSQL.MonitorMeasuredValues MM ON [MeasuredValueId] = MM.Id
  	WHERE TimeRaised &gt; @StartDate AND TimeRaised &lt; @EndDate AND (Severity = @Severity or @Severity=4) AND (State=@State or @State=5) AND A.SourceId in (SELECT * from ApexSQL.GetAllSourcesForMachine (@SourceID))
  		ORDER by Severity asc, TimeRaised</CommandText>
      </Query>
      <Fields>
        <Field Name="Source">
          <DataField>Source</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Metric">
          <DataField>Metric</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Severity">
          <DataField>Severity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Reviewed">
          <DataField>Reviewed</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="State">
          <DataField>State</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TimeRaised">
          <DataField>TimeRaised</DataField>
          <rd:TypeName>System.DateTimeOffset</rd:TypeName>
        </Field>
        <Field Name="Value">
          <DataField>Value</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="TimeResolved">
          <DataField>TimeResolved</DataField>
          <rd:TypeName>System.DateTimeOffset</rd:TypeName>
        </Field>
        <Field Name="UserResolved">
          <DataField>UserResolved</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AlertSeveritiesDataset">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT * FROM
(SELECT [Id],[Name]
  FROM [ApexSQLMonitor].[ApexSQL].[MonitorAlertSeverities]
UNION ALL
  SELECT 4 as [Id] , 'All' as [Name]) as Severity
ORDER BY ID Desc</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AlertStatesDataset">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT * FROM
(SELECT [Id],[Name]
  FROM [ApexSQLMonitor].[ApexSQL].[MonitorAlertStates]
UNION ALL
  SELECT 5 as [Id] , 'All' as [Name]) as States
ORDER BY ID Desc</CommandText>
      </Query>
      <Fields>
